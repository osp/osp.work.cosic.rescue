%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 400 400

/showpage { } def


/circle8 {
	/rot exch def
	/radius exch def
	/thick exch def
	/period exch def
	gsave
		2 2 scale
		0 0 translate
		thick setlinewidth

		rot rotate
		newpath
			0 0 radius 0 360 arc
		closepath clip newpath

		radius neg radius 1 add neg translate
		radius 5 add {
			gsave
				radius 24 div ceiling cvi {
					0 0 moveto
					24 24 24 -24 48 0 curveto
					stroke
					48 0 translate
				} repeat
			grestore
			0 period translate
		} repeat
	grestore
} def

80 80 translate
2 0.7 40 0 circle8

showpage
