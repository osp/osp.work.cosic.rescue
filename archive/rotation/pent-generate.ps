10 10 200 { %for
	/regx exch def
	regx 41 lt { %if
		/regy 1500 def
	} { %else
		regx 81 lt {
			/regy 1200 def
			/regx regx 40 sub def
		} {
			regx 121 lt {
				/regy 900 def
				/regx regx 80 sub def
			} { 
				regx 161 lt {
					/regy 600 def
					/regx regx 120 sub def
				} {
					regx 201 lt {
						/regy 300 def
						/regx regx 160 sub def
					} if
				} ifelse
			} ifelse
		} ifelse
	} ifelse
	regx 33 mul 150 sub regy single
} for

