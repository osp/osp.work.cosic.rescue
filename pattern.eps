    /x exch def 
    0 6 100 { 
        /y exch def 
        x y 2 0 360 arc closepath 
        fill 
    } for 
} for

%5 rotate

%0 6 100 { 
    %/x exch def 
    %0 6 100 { 
        %/y exch def 
        %x y 2 0 360 arc closepath 
        %fill 
    %} for 
%} for

%5 rotate

%0 6 100 { 
    %/x exch def 
